# Audio
persist.audio.button_jack.profile=volume
persist.audio.button_jack.switch=0
ro.vendor.audio.notification.single=true
tunnel.audio.encode=true
use.voice.path.for.pcm.voip=true

# Bluetooth
persist.vendor.btstack.aac_frm_ctl.enabled=true
ro.bluetooth.emb_wp_mode=false
ro.bluetooth.wipower=false

# Brightness Curve
persist.sys.brightness.low.gamma=true

# CNE
persist.vendor.cne.feature=1

# Ftrace
persist.debug.ftrace_events_disable=Yes

# Fingerprint
sys.fp.miui.token=0

# Deep Sleep
pm.sleep_mode=1
vendor.post_boot.parsed=1s

# IMS Debugging
persist.vendor.ims.disableADBLogs=1
persist.vendor.ims.disableDebugLogs=1
persist.vendor.ims.disableIMSLogs=1
persist.vendor.ims.disableQXDMLogs=1

# IOrap
ro.iorapd.enable=true
persist.device_config.runtime_native_boot.iorap_perfetto_enable=true
persist.device_config.runtime_native_boot.iorap_readahead_enable=true

# Fwk-Detect
ro.vendor.qti.va_aosp.support=1

# Hint Manager
debug.sf.enable_adpf_cpu_hint=true
debug.hwui.use_hint_manager=true
debug.hwui.target_cpu_time_percent=50

# HWUI
debug.sf.latch_unsignaled=0
ro.hwui.texture_cache_size=72
ro.hwui.layer_cache_size=48
ro.hwui.r_buffer_cache_size=8
ro.hwui.max_texture_allocation_size=209715200
ro.hwui.path_cache_size=32
ro.hwui.gradient_cache_size=1
ro.hwui.drop_shadow_cache_size=6
ro.hwui.texture_cache_flushrate=0.4
ro.hwui.text_small_cache_width=1024
ro.hwui.text_small_cache_height=1024
ro.hwui.text_large_cache_width=2048
ro.hwui.text_large_cache_height=1024
debug.hwui.skia_atrace_enabled=false
config.disable_rtt=true

# Media
media.settings.xml=/vendor/etc/media_profiles_khaje_v0.xml
media.aac_51_output_enabled=true
media.stagefright.enable-aac=true
media.stagefright.enable-fma2dp=true
media.stagefright.enable-http=true
media.stagefright.enable-player=true
media.stagefright.enable-qcp=true
media.stagefright.enable-scan=true
media.stagefright.thumbnail.prefer_hw_codecs=true

# NTP
persist.backup.ntpServer="0.pool.ntp.org"

# Radio
DEVICE_PROVISIONED=1
persist.rcs.supported=1
persist.vendor.radio.atfwd.start=true
ril.subscription.types=NV,RUIM
ro.telephony.default_network=22,22
telephony.lteOnCdmaDevice=1

# SdCardFS
persist.sys.fuse.passthrough.enable=true

# Soc
ro.soc.manufacturer=QTI

# Sensors
persist.vendor.sensors.enable.mag_filter=true

# SSR
persist.vendor.ssr.restart_level=ALL_ENABLE


